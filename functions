import pandas as pd
import json
import urllib.request
import re
import os.path
import random
import sys
import io

def get_title_from_url(url):
    response = urllib.request.urlopen(url).read().decode("utf-8")
    title = re.search(r'partName:"(.*)",', response).group(1).split('ï¼š')[0]
    return title
